[flake8]
max-line-length = 127
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    .venv,
    venv,
    env,
    .env,
    .tox,
    .pytest_cache,
    __pycache__,
    *.pyc,
    .mypy_cache,
    .coverage,
    htmlcov,
    .idea,
    .vscode,
    mlruns,
    notebooks,
    reports
ignore = 
    E203,
    E266,
    E501,
    W503,
    F403,
    F401
per-file-ignores = 
    __init__.py:F401
    tests/*:F401,F811